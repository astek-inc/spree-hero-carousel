<%= render partial: 'spree/admin/shared/hero_carousel_tabs', locals: {current: :hero_carousel_items} %>

<h2 style="font-size: 20px;"><%= Spree.t(:editing_hero_carousel_item) %></h2>

<%= form_for [:admin, @hero_carousel, @hero_carousel_item], html: {multipart: true} do |f| %>
    <fieldset data-hook="edit_hero_carousel_item">
      <%= render partial: 'form', locals: {f: f} %>
      <%= render partial: 'spree/admin/shared/edit_resource_links', locals: {collection_url: spree.admin_hero_carousel_hero_carousel_items_url(@hero_carousel) } %>
    </fieldset>
<% end %>

